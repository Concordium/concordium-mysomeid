  "scripts": {
    "start": "nodemon --watch 'src/**/*' -e 'tsx,ts,js' --exec yarn build",
    "start:popup": "nodemon --watch 'src-popup/**/*' -e tsx,ts,js --exec yarn build:popup",
    "start:popup:browser": "yarn add ansi-html-community && react-app-rewired start",
    "start:content": "nodemon --watch 'src-content/**/*' -e tsx,ts,js --exec yarn build:content",
    "build": "yarn build:popup && yarn build:widget && yarn build:background && yarn build:content && yarn build:injected && yes | cp assets/icon.png build && yes | cp assets/manifest.json build",
    "build:popup": "INLINE_RUNTIME_CHUNK=false PUBLIC_URL=/popup/ react-app-rewired build",
    "build:content": "webpack --config webpack.content.config.js",
    "build:widget": "webpack --config webpack.widget.config.js",
    "build:background": "tsc --project tsconfig-background.json",
    "build:injected": "webpack --config webpack.injected.config.js",
    "watch:widget": "nodemon --watch src-widget/**/* -e 'tsx,ts,js' --exec yarn build:widget",
    "test": "react-app-rewired test",
    "package": "/Applications/Google\\ Chrome.app/Contents/MacOS/Google\\ Chrome --pack-extension=./build --pack-extension-key=./privkey2.pem",
    "host-test": "http-server-spa test-page index.html 8082",
    "host-widget": "http-server-spa build/widget index.html 1234",
    "lint": "npx tslint -c tslint.json 'src-*/**/*.ts'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
