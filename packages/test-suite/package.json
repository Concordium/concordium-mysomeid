{
  "name": "test-suite",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test:integration": "jest --config jest.integration.config.js --detectOpenHandles",
    "test:e2e": "jest --config jest.e2e.config.js  --detectOpenHandles",
    "mockli": "npx ts-node --project mock-server-tsconfig.json tests/mock-server.ts",
    "browser": "npx ts-node utils/open-browser.ts",
    "pack-user-data": "tar -czvf user-data.tar.gz ./user-data",
    "test:docker:integration:build": "docker compose build",
    "test:docker:integration:run": "docker compose run test_integration",
    "test:docker:e2e:build": "docker compose build",
    "test:docker:e2e:run": "docker compose run test_e2e"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.15.0",
    "hostile": "^1.3.3",
    "hosts-file": "^0.0.2",
    "jest": "^29.5.0",
    "jest-puppeteer": "^8.0.5",
    "puppeteer": "^19.7.3",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "express": "^4.18.2",
    "jimp": "^0.22.7",
    "localtunnel": "^2.0.2",
    "multer": "^1.4.5-lts.1"
  }
}
